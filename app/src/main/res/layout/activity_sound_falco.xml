<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SoundActivityFalco">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <Space
            android:id="@+id/align"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerInParent="true"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/falco_palette"
            android:layout_centerHorizontal="true"
            android:id="@+id/falco_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/AUDIENCE_CHEER"
            android:id="@+id/falco_cheer_button"
            app:soundID="@string/FALCO_CHEER"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/VICTORY1"
            android:id="@+id/falco_mission_complete_button"
            app:soundID="@string/FALCO_MISSION_COMPLETE"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/TAUNT"
            android:id="@+id/falco_taunt_button"
            app:soundID="@string/FALCO_TAUNT"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_cheer_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH1"
            android:id="@+id/falco_smash1_button"
            app:soundID="@string/FALCO_SMASH1"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_mission_complete_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH2"
            android:id="@+id/falco_smash2_button"
            app:soundID="@string/FALCO_SMASH2"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_taunt_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH3"
            android:id="@+id/falco_smash3_button"
            app:soundID="@string/FALCO_SMASH3"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_smash1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH4"
            android:id="@+id/falco_smash4_button"
            app:soundID="@string/FALCO_SMASH4"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_smash2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH5"
            android:id="@+id/falco_smash5_button"
            app:soundID="@string/FALCO_SMASH5"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_smash3_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SPOT_DODGE"
            android:id="@+id/falco_spot_dodge_button"
            app:soundID="@string/FALCO_SPOT_DODGE"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_smash4_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/NEUTRAL_SPECIAL"
            android:id="@+id/falco_laser_button"
            app:soundID="@string/FALCO_LASER"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_smash5_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SIDE_SPECIAL"
            android:id="@+id/falco_phantasm_button"
            app:soundID="@string/FALCO_PHANTASM"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_spot_dodge_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/UP_SPECIAL"
            android:id="@+id/falco_firebird_button"
            app:soundID="@string/FALCO_FIREBIRD"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_laser_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DOWN_SPECIAL"
            android:id="@+id/falco_shine_button"
            app:soundID="@string/FALCO_SHINE"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_phantasm_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE1"
            android:id="@+id/falco_damage1_button"
            app:soundID="@string/FALCO_DAMAGE1"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_firebird_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE2"
            android:id="@+id/falco_damage2_button"
            app:soundID="@string/FALCO_DAMAGE2"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_shine_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE3"
            android:id="@+id/falco_damage3_button"
            app:soundID="@string/FALCO_DAMAGE3"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_damage1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEATH1"
            android:id="@+id/falco_death1_button"
            app:soundID="@string/FALCO_DEATH1"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_damage2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEATH2"
            android:id="@+id/falco_death2_button"
            app:soundID="@string/FALCO_DEATH2"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_damage3_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEATH3"
            android:id="@+id/falco_death3_button"
            app:soundID="@string/FALCO_DEATH3"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_death1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/STAR_KO"
            android:id="@+id/falco_off_top_button"
            app:soundID="@string/FALCO_STAR_KO"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_death2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DOUBLE_JUMP"
            android:id="@+id/falco_double_jump_button"
            app:soundID="@string/FALCO_DOUBLE_JUMP"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_death3_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/GUN_DRAW"
            android:id="@+id/falco_gun_draw_button"
            app:soundID="@string/FALCO_GUN_DRAW"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_off_top_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/GUN_WITHDRAWAL"
            android:id="@+id/falco_gun_withdrawal_button"
            app:soundID="@string/FALCO_GUN_WITHDRAWAL"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/falco_double_jump_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MULTISHINE"
            android:id="@+id/falco_multishine_button"
            app:soundID="@string/FALCO_MULTISHINE"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/falco_gun_draw_button"/>
    </RelativeLayout>
</ScrollView>