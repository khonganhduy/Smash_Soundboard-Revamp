<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SoundActivityMarth">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <Space
            android:id="@+id/align"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerInParent="true"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/marth_palette"
            android:layout_centerHorizontal="true"
            android:id="@+id/marth_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/AUDIENCE_CHEER"
            android:id="@+id/marth_cheer_button"
            app:soundID="@string/MARTH_CHEER"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/VICTORY1"
            android:id="@+id/marth_victory_button"
            app:soundID="@string/MARTH_VICTORY"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_banner"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/TAUNT"
            android:id="@+id/marth_taunt_button"
            app:soundID="@string/MARTH_TAUNT"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_cheer_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH1"
            android:id="@+id/marth_smash1_button"
            app:soundID="@string/MARTH_SMASH1"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_victory_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH2"
            android:id="@+id/marth_smash2_button"
            app:soundID="@string/MARTH_SMASH2"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_taunt_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH3"
            android:id="@+id/marth_smash3_button"
            app:soundID="@string/MARTH_SMASH3"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_smash1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH4"
            android:id="@+id/marth_smash4_button"
            app:soundID="@string/MARTH_SMASH4"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_smash2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SMASH5"
            android:id="@+id/marth_smash5_button"
            app:soundID="@string/MARTH_SMASH5"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_smash3_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SPOT_DODGE"
            android:id="@+id/marth_spot_dodge_button"
            app:soundID="@string/MARTH_SPOT_DODGE"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_smash4_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/NEUTRAL_SPECIAL"
            android:id="@+id/marth_neutral_b_button"
            app:soundID="@string/MARTH_NEUTRAL_B"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_smash5_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SIDE_SPECIAL"
            android:id="@+id/marth_side_b_button"
            app:soundID="@string/MARTH_SIDE_B"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_spot_dodge_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/UP_SPECIAL"
            android:id="@+id/marth_up_b_button"
            app:soundID="@string/MARTH_UP_B"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_neutral_b_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DOWN_SPECIAL"
            android:id="@+id/marth_down_b_button"
            app:soundID="@string/MARTH_DOWN_B"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_side_b_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MORE_COUNTERS"
            android:id="@+id/marth_counter1_button"
            app:soundID="@string/MARTH_COUNTER1"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_up_b_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MORE_COUNTERS"
            android:id="@+id/marth_counter2_button"
            app:soundID="@string/MARTH_COUNTER2"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_down_b_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE1"
            android:id="@+id/marth_damage1_button"
            app:soundID="@string/MARTH_DAMAGE1"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_counter1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE2"
            android:id="@+id/marth_damage2_button"
            app:soundID="@string/MARTH_DAMAGE2"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_counter2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DAMAGE3"
            android:id="@+id/marth_damage3_button"
            app:soundID="@string/MARTH_DAMAGE3"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_damage1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEATH1"
            android:id="@+id/marth_death1_button"
            app:soundID="@string/MARTH_DEATH1"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_damage2_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEATH2"
            android:id="@+id/marth_death2_button"
            app:soundID="@string/MARTH_DEATH2"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_damage3_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/STAR_KO"
            android:id="@+id/marth_off_top_button"
            app:soundID="@string/MARTH_STAR_KO"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/align"
            android:layout_below="@id/marth_death1_button"/>

        <memes.smashsoundboard.SoundButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/QUOTE"
            android:id="@+id/marth_quote_button"
            app:soundID="@string/MARTH_QUOTE"
            android:layout_alignParentRight="true"
            android:layout_alignLeft="@id/align"
            android:layout_below="@id/marth_death2_button"/>


    </RelativeLayout>
</ScrollView>