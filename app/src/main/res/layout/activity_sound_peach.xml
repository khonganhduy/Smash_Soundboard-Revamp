<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SoundActivityPeach">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/peach_banner"
            android:background="@drawable/peach_palette" />

        <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*">

            <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <memes.smashsoundboard.SoundButton
                android:text="@string/audience_cheer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_cheer"
                app:soundID="@string/peach_cheer"/>

            <memes.smashsoundboard.SoundButton
                android:text="@string/victory"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_victory1"
                app:soundID="@string/peach_victory"/>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <memes.smashsoundboard.SoundButton
                android:text="@string/victory1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_victory2"
                app:soundID="@string/peach_victory2"/>

            <memes.smashsoundboard.SoundButton
                android:text="@string/taunt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_taunt"
                app:soundID="@string/peach_taunt"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <memes.smashsoundboard.SoundButton
                android:text="@string/smash1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_smash1"
                app:soundID="@string/peach_smash1"/>

            <memes.smashsoundboard.SoundButton
                android:text="@string/smash2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_smash2"
                app:soundID="@string/peach_smash2"/>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <memes.smashsoundboard.SoundButton
                android:text="@string/smash3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_smash3"
                app:soundID="@string/peach_smash3"/>

            <memes.smashsoundboard.SoundButton
                android:text="@string/smash4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/peach_smash4"
                app:soundID="@string/peach_smash4"/>
        </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/smash5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_smash5"
                    app:soundID="@string/peach_smash5"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/peach_frying_pan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_pan"
                    app:soundID="@string/peach_pan"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/peach_club"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_golf_club"
                    app:soundID="@string/peach_golf_club"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/peach_racket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_tennis_racket"
                    app:soundID="@string/peach_tennis_racket"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/jump"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_jump"
                    app:soundID="@string/peach_jump"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/double_jump"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_double_jump"
                    app:soundID="@string/peach_double_jump"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/neutral_special"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_toad"
                    app:soundID="@string/peach_toad"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/side_special"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_bomber"
                    app:soundID="@string/peach_bomber"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/down_special"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_pull"
                    app:soundID="@string/peach_pull"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/up_special"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_parasol"
                    app:soundID="@string/peach_parasol"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/damage1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_damage"
                    app:soundID="@string/peach_damage"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/death1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_death"
                    app:soundID="@string/peach_death"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <memes.smashsoundboard.SoundButton
                    android:text="@string/star_ko"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_star_ko"
                    app:soundID="@string/peach_star_ko"/>

                <memes.smashsoundboard.SoundButton
                    android:text="@string/screen_ko"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/peach_screen_ko"
                    app:soundID="@string/peach_screen_ko"/>

            </TableRow>
    </TableLayout>
    </LinearLayout>
</ScrollView>